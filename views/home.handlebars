<div class="container">


    <div>
        <h1>All Articles:</h1>
    </div>

    <div>
        <ul>
            {{#each articles}}
            <li>
                <h2>{{this.title}}</h2>
                <h3>Written by {{this.author}}</h3>
                <h4>Posted on {{this.timestamp}}</h4>
                <p>{{this.content}}</p>
            </li>
            {{/each}}
        </ul>
    </div>
    
</div>
